<div class="container">
  <div class="page-header">
    <h1>Frequently Asked Questions</h1>
  </div>
  <div class="row">


    <div class="col-md-6">
      <ul>
        <li><%= link_to @question1.question, "#"+@question1.slug %></li>
        <li><%= link_to @question2.question, "#"+@question2.slug %></li>
        <li><%= link_to @question3.question, "#"+@question3.slug %></li>
      </ul>
      <% @questionArray.each do |stuff| %>
      <p>
        <strong> <a id="<%= stuff.slug %>"></a> <%= stuff.question %>""</strong></p>
      <p>
        <%= stuff.answer %>
      </p>
      <% end %>
    </div>
  </div>
